<div class="row">
    <div class="form-group col-md-6">
        <label for="name">Name</label>
        <input required value="<%= project.name %>" type="text" name="name" id="name" class="form-control">
    </div>

    <div class="form-group col-md-6">
        <label for="repoUrl">repoUrl</label>
        <input required name="repoUrl" id="repoUrl" class="form-control"><%= project.repoUrl %></input>
    </div>
</div>

<div class="form-group">
    <label for="markdown">Markdown</label>
    <textarea required name="markdown" id="markdown" class="form-control"><%= project.markdown %></textarea>
</div>



<a href="/" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>